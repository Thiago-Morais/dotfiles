    "mpd": {
        "max-length": 25,
        "format": "<span foreground='#bb9af7'>ÔÄÅ</span> {title}",
        "format-paused": "ÔÄÅ {title}",
        "format-stopped": "<span foreground='#bb9af7'>ÔÄÅ</span>",
